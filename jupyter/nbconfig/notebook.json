{
  "Cell": {
    "cm_config": {
      "lineNumbers": false
    }
  },
  "Notebook": {
    "Header": false,
    "Toolbar": true
  },
  "keys": {
    "command": {
      "bind": {
        "cmd-shift-e": "jupyter-notebook:close-and-halt",
        "f,e": "jupyter-notebook:restart-kernel-and-run-all-cells",
        "ctrl-f,h": "jupyter-notebook:close-and-halt",
        "ctrl-f,r": "jupyter-notebook:restart-kernel-and-run-all-cells",
        "cmd-shift-r": "jupyter-notebook:restart-kernel-and-run-all-cells"
      }
    }
  },
  "load_extensions": {
    "vim_binding/vim_binding": true,
    "nbextensions_configurator/config_menu/main": true
  }
}